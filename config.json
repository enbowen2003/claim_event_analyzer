{
  "io": {
    "delimiter": "|",
    "encoding": "utf-8-sig",
    "datetime_formats": ["%m/%d/%Y %H:%M:%S", "%m/%d/%Y %H:%M"],
    "header_match": "normalized",
    "sniff_delimiter": false
  },
  "grouping": {
    "base_len": 10
  },
  "fields": {
    "pk": "EVENT_ID",
    "prev_pk": "PREV_EVENT_ID",
    "clm01_full": "CLM01",
    "ref_f8": "REF_F8",
    "system_state": "SYS_STATE",
    "system_status": "SYS_STATUS",
    "clm0503": "CLM0503",
    "system_clm0503": "SYS_CLM0503",
    "cms_icn": "CMS_ICN",
    "cms_out_icn": "CMS_OUT_ICN",
    "created_at": "CREATE_DT",
    "create_by": "CREATE_BY",
    "update_by": "UPDATE_BY"
  },
  "rules": {
    "accepted_status_values": ["CMS ACCEPTED", "ACCEPTED"],
    "rejected_status_values": ["REJECTED"],
    "locked_status_values": ["LOCKED"],
    "submitted_state_values": ["SUBMITTED"],

    "manual_user_prefixes": ["INC"]
  }
}
